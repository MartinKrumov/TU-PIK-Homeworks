#define _CRT_SECURE_NO_WARNINGS 
#include <stdio.h> 
#include <conio.h> 
#include <limits.h> 

int main()
{
	int number, max = INT_MIN;
	do
	{
		printf("Enter number: ");
		scanf("%d", &number);
		if (max < number)
		{
			max = number;
		}
	} while (number != 0);

	printf("The largest number is: %d", max);

	_getch();
	return 0;
}
